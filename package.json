  },
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.8.15"
  },
  "devDependencies": {
    "chai": ">=1.4.0",
    "mocha": ">=1.7.4",
    "coffee-script": "~1.4.0"
  },
  "scripts": {
    "pretest": "coffee --bare -o js coffee",
    "test": "mocha --compilers coffee:coffee-script -R spec --ui bdd --colors test/test.coffee"
  }
