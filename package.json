{
  "name": "coffee-espresso-two-shots",
  "preferGlobal": "true",
  "version": "0.0.2",
  "author": "Nino Paolo Amarillento <oo70vd@gmail.com>",
  "description": "Converting HAML and SASS template to coffee-template and coffee-stylesheet respectively.",
  "keywords": [
    "coffee-template",
    "coffee-stylesheet",
    "coffee-shop",
    "haml",
    "sass"
